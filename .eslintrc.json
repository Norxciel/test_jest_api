{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
         "jest/globals": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
